{
	"name": "@andresmarpz/favicons",
	"author": "andresmarpz",
	"version": "1.0.0",
	"description": "Utility package to get favicons from the web.",
	"main": "./dist/index.js",
	"module": "./dist/index.modern.mjs",
	"types": "./dist/index.d.ts",
	"source": "./src/index.ts",
	"files": [
		"dist"
	],
	"license": "GNU3",
	"scripts": {
