		"fetch"
	],
	"homepage": "https://github.com/andresmarpz/favicons#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/andresmarpz/favicons.git"
	},
	"bugs": {
		"url": "https://github.com/andresmarpz/favicons/issues",
		"email": "hello@andrs.me"
	},
	"type": "module",
	"source": "src/index.ts",
	"main": "dist/index.cjs",
	"module": "dist/index.modern.mjs",
	"exports": {
