		"jest": "^29.0.3",
		"microbundle": "^0.15.1",
		"nodemon": "^2.0.19",
		"ts-jest": "^29.0.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.3"
	},
	"dependencies": {
		"cheerio": "^1.0.0-rc.12",
		"node-fetch": "^2.6.7",
		"puppeteer": "^17.1.3"
