{
	"name": "@andresmarpz/favicons",
	"author": "Andrés Martínez <hello@andrs.me>",
	"version": "1.0.8",
	"description": "Utility package to get favicons from the web.",
	"keywords": [
		"favicon",
		"favicons",
		"get",
		"node"
	],
	"type": "module",
	"source": "src/index.ts",
	"main": "dist/index.cjs",
	"module": "dist/index.modern.mjs",
	"homepage": "https://github.com/andresmarpz/favicons#readme",
	"exports": {
		"types": "./dist/index.d.ts",
		"require": "./dist/index.cjs",
		"default": "./dist/index.modern.mjs"
	},
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"license": "GNU3",
	"scripts": {
		"build": "microbundle build --compress --no-sourcemap --format modern,cjs --tsconfig tsconfig.build.json --target node",
		"test": "jest --forceExit",
		"dev": "nodemon src/index.ts",
		"start": "ts-node src/index.ts",
		"prepublish": "npm test && tsc",
		"prepack": "npm run build",
		"postpack": "npm link"
	},
	"devDependencies": {
		"@types/jest": "^29.0.2",
		"@types/jsdom": "^20.0.0",
		"@types/puppeteer": "^5.4.6",
		"jest": "^29.0.3",
		"microbundle": "^0.15.1",
		"nodemon": "^2.0.19",
		"ts-jest": "^29.0.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.3"
	},
	"dependencies": {
		"cheerio": "^1.0.0-rc.12"
	}
}
